diff --git a/tmp/Docker - 7.2.3 b/tmp/Docker - 7.2.3.patched
index 236ef3d..5172e10 100644
--- a/usr/local/emhttp/plugins/dynamix.docker.manager/system/Docker
+++ b/usr/local/emhttp/plugins/dynamix.docker.manager/system/Docker	
@@ -8,6 +8,24 @@ if [[ -z $ACTIVE_CONTAINERS ]]; then
   exit
 fi
 
+# Optionally filter out compose-managed containers when compose.manager requests it
+HIDE_COMPOSE="false"
+if [ -f /boot/config/plugins/compose.manager/compose.manager.cfg ]; then
+  HIDE_COMPOSE=$(awk -F'=' '/^HIDE_COMPOSE_FROM_DOCKER/ {gsub(/"/,"",$2); print $2}' /boot/config/plugins/compose.manager/compose.manager.cfg)
+fi
+if [ "$HIDE_COMPOSE" = "true" ]; then
+  FILTERED=""
+  for c in ${ACTIVE_CONTAINERS}; do
+    MGR=$(docker inspect --format '{{index .Config.Labels "net.unraid.docker.managed"}}' $c 2>/dev/null || true)
+    COMP=$(docker inspect --format '{{index .Config.Labels "com.docker.compose.project"}}' $c 2>/dev/null || true)
+    if [ "$MGR" = "composeman" ] || [ -n "$COMP" ]; then
+      continue
+    fi
+    FILTERED="$FILTERED $c"
+  done
+  ACTIVE_CONTAINERS="$FILTERED"
+fi
+
 # Get all relevant memory entries from containers
 for container in ${ACTIVE_CONTAINERS} ; do
   CONT_MEMORY="$(cat /sys/fs/cgroup/docker/${container}/memory.stat 2>/dev/null | grep -Ew "anon|kernel|kernel_stack|pagetables|sec_pagetables|percpu|sock|vmalloc|shmem" | awk '{print $2}')"
